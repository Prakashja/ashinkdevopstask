pipeline {
    agent any

    stages{
        stage("ansible-config"){
            steps{
                script {
                    sh '''
                    ls -ltr 
                    pwd
                    cd ansible
                    chmod 600 DevOps-Key.pem
                    export ANSIBLE_HOST_KEY_CHECKING=False
                    ansible -i inventory  -m ping dashboard
                    '''
                }
            }
        }
    }
}
